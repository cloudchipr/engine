{
  "ebs-collect": {
    "policies": [
      {
        "name": "ebs-collect",
        "resource": "ebs"
      }
    ]
  },
  "ebs-clean": {
    "policies": [
      {
        "name": "ebs-clean",
        "resource": "ebs",
        "actions": [
          "delete"
        ]
      }
    ]
  },
  "ec2-collect": {
    "policies": [
      {
        "name": "ec2-collect",
        "resource": "ec2",
        "filters": [
          {
            "State.Name": "running"
          }
        ]
      }
    ]
  },
  "ec2-clean": {
    "policies": [
      {
        "name": "ec2-clean",
        "resource": "ec2",
        "actions": [
          {
            "type": "terminate",
            "force": true
          }
        ]
      }
    ]
  },
  "elb-collect": {
    "policies": [
      {
        "name": "elb-collect",
        "resource": "elb"
      }
    ]
  },
  "elb-clean": {
    "policies": [
      {
        "name": "elb-clean",
        "resource": "elb",
        "actions": [
          "delete"
        ]
      }
    ]
  },
  "nlb-collect": {
    "policies": [
      {
        "name": "nlb-collect",
        "resource": "app-elb",
        "filters": [
          {
            "Type": "network"
          }
        ]
      }
    ]
  },
  "nlb-collect-by-instance": {
    "policies": [
      {
        "name": "nlb-collect-by-instance",
        "resource": "app-elb",
        "filters": [
          {
            "Type": "network"
          },
          {
            "and": [
              {
                "type": "value",
                "key": "LoadBalancerArn",
                "op": "in",
                "value": ""
              }
            ]
          }
        ]
      }
    ]
  },
  "nlb-clean": {
    "policies": [
      {
        "name": "nlb-clean",
        "resource": "app-elb",
        "filters": [
          {
            "Type": "network"
          }
        ],
        "actions": [
          "delete"
        ]
      }
    ]
  },
  "alb-collect": {
    "policies": [
      {
        "name": "alb-collect",
        "resource": "app-elb",
        "filters": [
          {
            "Type": "application"
          }
        ]
      }
    ]
  },
  "alb-collect-by-instance": {
  "policies": [
    {
      "name": "alb-collect-by-instance",
      "resource": "app-elb",
      "filters": [
        {
          "Type": "application"
        },
        {
          "and": [
            {
              "type": "value",
              "key": "LoadBalancerArn",
              "op": "in",
              "value": ""
            }
          ]
        }
      ]
    }
  ]
},
  "alb-clean": {
    "policies": [
      {
        "name": "alb-clean",
        "resource": "app-elb",
        "filters": [
          {
            "Type": "application"
          }
        ],
        "actions": [
          "delete"
        ]
      }
    ]
  },
  "target-group-collect": {
    "policies": [
      {
        "name": "target-group-collect",
        "resource": "app-elb-target-group"
      }
    ]
  },
  "eip-collect": {
    "policies": [
      {
        "name": "eip-collect",
        "resource": "network-addr"
      }
    ]
  },
  "eip-clean": {
    "policies": [
      {
        "name": "eip-clean",
        "resource": "network-addr",
        "actions": [
          {
            "type": "release"
          }
        ]
      }
    ]
  },
  "rds-collect": {
    "policies": [
      {
        "name": "rds-collect",
        "resource": "rds"
      }
    ]
  },
  "rds-clean": {
    "policies": [
      {
        "name": "rds-clean",
        "resource": "rds",
        "actions": [
          {
            "type": "delete",
            "skip-snapshot": true
          }
        ]
      }
    ]
  }
}
